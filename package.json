{
  "name": "elm-graphql",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "build": "node cli/build.js ; tsc ; yarn build:elm:opt ; cp cli/package.json dist/package.json",
    "build:elm": "elm-codegen install; elm make src/Generate.elm --output=./dist/generators/schema.js",
    "build:elm:opt": "elm-codegen install; elm-optimize-level-2 src/Generate.elm --output=./dist/generators/schema.js",
    "elm-gql": "node ./dist/index.js",
    "elm-gql:test": "node ./dist/index.js --namespace=Api2 --schema=https://api.blissfully.com/prod/graphql --gql=gql-ops",
    "elm-gql:test-json-schema-file": "node ./dist/index.js --namespace=Api2 --schema=schema.json --gql=gql-ops --force",
    "elm-gql:run": "node ./dist/index.js --schema=schema.json --gql=gql-ops --force",
    "test": "elm-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Blissfully/elm-gql.git"
  },
  "author": "",
  "license": "BSD-3",
  "bugs": {
    "url": "https://github.com/Blissfully/elm-gql/issues"
  },
  "homepage": "https://github.com/Blissfully/elm-gql#readme",
  "devDependencies": {
    "elm-codegen": "^0.2.0",
    "elm-doc-preview": "^5.0.5",
    "elm-optimize-level-2": "^0.3.4",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@types/node": "^15.0.3",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.1"
  }
}
